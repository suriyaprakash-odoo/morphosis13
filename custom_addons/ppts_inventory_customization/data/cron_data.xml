<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="ir_cron_send_daily_action_plan" model="ir.cron">
			<field name="name">Daily Action Plan to Supervisors</field>
			<field name="model_id" ref="model_project_container"/>
			<field name="type">ir.actions.server</field>
			<field name="state">code</field>
			<field name="code">model.send_daily_action_plan()</field>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="numbercall">-1</field>
		</record>

		<record id="ir_cron_set_container_inprogress" model="ir.cron">
			<field name="name">Set Containers In-progress</field>
			<field name="model_id" ref="model_project_container"/>
			<field name="type">ir.actions.server</field>
			<field name="state">code</field>
			<field name="code">model.set_containers_inprogress()</field>
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="numbercall">-1</field>
		</record>


	</data>
</odoo>
