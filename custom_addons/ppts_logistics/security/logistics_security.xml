<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

	    <record model="ir.module.category" id="module_category_operations_logistics">
	        <field name="name">Logistics</field>
	        <field name="sequence">11</field>
	    </record>

	    <record id="group_logistics_user" model="res.groups">
	        <field name="name">User</field>
	        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
	        <field name="category_id" ref="module_category_operations_logistics"/>
	    </record>

	    <record id="group_logistics_manager" model="res.groups">
	        <field name="name">Manager</field>
	        <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('group_logistics_user'))]"/>
	        <field name="category_id" ref="module_category_operations_logistics"/>
	    </record>

	    <record model="ir.module.category" id="module_category_operations_res_ports">
	        <field name="name">Sea Ports</field>
	        <field name="sequence">12</field>
	    </record>

	    <record id="group_res_ports_user" model="res.groups">
	        <field name="name">User</field>
	        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
	        <field name="category_id" ref="module_category_operations_res_ports"/>
	    </record>

	    <record id="group_res_ports_manager" model="res.groups">
	        <field name="name">Manager</field>
	        <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('group_res_ports_user'))]"/>
	        <field name="category_id" ref="module_category_operations_res_ports"/>
	    </record>

	    <record id="project_logistics_user_record_rule_new" model="ir.rule">
	        <field name="name">project.logistics.user</field>
	        <field name="model_id" ref="ppts_project_entries.model_project_entries"/>
	        <field name="domain_force">[(1, '=', 1)]</field>
	        <field name="groups" eval="[(4, ref('ppts_logistics.group_logistics_user'))]"/>
	        <field name="perm_create" eval="0"/>
	        <field name="perm_write" eval="1"/>
	        <field name="perm_unlink" eval="0"/>
	        <field name="perm_read" eval="1"/>
	    </record>

	    <record id="project_logistics_manager_record_rule_new" model="ir.rule">
	        <field name="name">project.logistics.user</field>
	        <field name="model_id" ref="ppts_project_entries.model_project_entries"/>
	        <field name="domain_force">[(1, '=', 1)]</field>
	        <field name="groups" eval="[(4, ref('ppts_logistics.group_logistics_manager'))]"/>
	        <field name="perm_create" eval="0"/>
	        <field name="perm_write" eval="1"/>
	        <field name="perm_unlink" eval="0"/>
	        <field name="perm_read" eval="1"/>
	    </record>

	</data>
</odoo>
