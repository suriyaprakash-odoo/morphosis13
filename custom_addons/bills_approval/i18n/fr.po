#. module: bills_approval
#: model:mail.template,body_html:bills_approval.template_send_for_approval
msgid ""
"\n"
"\t\t\t\t\n"
"                    <p>Hi,</p>\n"
"\n"
"                    <p>There is a bill waiting for your approval to process the payment. Find the details below.\n"
"                    <br/>\n"
"                    <br/>\n"
"                    % if object.invoice_origin\n"
"                    \t<p>Purchase Order: <strong>${object.invoice_origin}</strong> </p>\n"
"                    % endif\n"
"                    <br/>\n"
"                    <p>Vendor: <strong>${object.partner_id.name}</strong></p>\n"
"                    <br/>\n"
"                    <p>Total Amount: <strong>${format_amount(object.amount_total, object.currency_id)}</p>\n"
"                    <br/>\n"
"\t\t\t\t\t<br/>\n"
"\n"
"\t\t\t\t\t<a href=\"${ctx.get('url')}\"\n"
"\t\t\t\t\t\tstyle=\"background-color: #875A7B; padding: 10px; text-decoration: none; color: #fff; border-radius: 5px; font-size: 15px;text-transform:uppercase;\">Click to Open</a>\n"
"\t\t\t\t\t<br/>\n"
"\t\t\t\t\t<br/>\n"
"\t\t\t\t\t<br/>\n"
"\t\t\t\t\t<br/>\n"
"\n"
"                    <p>Thanks,</p>\n"
"                    <p>${object.company_id.name}</p>\n"
"                    <p><i>Morphosis Group</i></p>\n"
"                \n"
"\t\t\t"
